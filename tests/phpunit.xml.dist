<?xml version="1.0" encoding="UTF-8"?>
<!-- TODO set checkForUnintentionallyCoveredCode="true" once https://www.drupal.org/node/2626832 is resolved. -->
<!-- PHPUnit expects functional tests to be run with either a privileged user
 or your current system user. See core/tests/README.md and
 https://www.drupal.org/node/2116263 for details.
-->
<phpunit bootstrap="tests/bootstrap.php" colors="true"
         beStrictAboutTestsThatDoNotTestAnything="true"
         beStrictAboutOutputDuringTests="true"
         beStrictAboutChangesToGlobalState="true"
         printerClass="\Drupal\Tests\Listeners\HtmlOutputPrinter">
  <php>
    <!-- Set error reporting to E_ALL. -->
    <ini name="error_reporting" value="32767"/>
    <!-- Do not limit the amount of memory tests take to run. -->
    <ini name="memory_limit" value="-1"/>
    <!-- Example SIMPLETEST_BASE_URL value: http://localhost -->
    <env name="SIMPLETEST_BASE_URL" value="http://127.0.0.1"/>
    <!-- Example SIMPLETEST_DB value: mysql://username:password@localhost/databasename#table_prefix -->
    <env name="SIMPLETEST_DB" value="sqlsrv://sa:Password12!@localhost/mydrupalsite?dbo"/>
    <!-- Example BROWSERTEST_OUTPUT_DIRECTORY value: /path/to/webroot/sites/simpletest/browser_output -->
    <env name="BROWSERTEST_OUTPUT_DIRECTORY" value="WEB_DIR/sites/simpletest/browser_output"/>
    <!-- To have browsertest output use an alternative base URL. For example if
     SIMPLETEST_BASE_URL is an internal DDEV URL, you can set this to the
     external DDev URL so you can follow the links directly.
    -->
    <env name="BROWSERTEST_OUTPUT_BASE_URL" value=""/>
    <!-- To disable deprecation testing completely uncomment the next line. -->
    <env name="SYMFONY_DEPRECATIONS_HELPER" value="disabled"/>

    <!-- Example for changing the driver class for mink tests MINK_DRIVER_CLASS value: 'Drupal\FunctionalJavascriptTests\DrupalSelenium2Driver' -->
    <env name="MINK_DRIVER_CLASS" value=''/>
    <!-- Example for changing the driver args to mink tests MINK_DRIVER_ARGS value: '["http://127.0.0.1:8510"]' -->
    <env name="MINK_DRIVER_ARGS" value=''/>
    <!-- Example for changing the driver args to phantomjs tests MINK_DRIVER_ARGS_PHANTOMJS value: '["http://127.0.0.1:8510"]' -->
    <env name="MINK_DRIVER_ARGS_PHANTOMJS" value=''/>
    <!-- Example for changing the driver args to webdriver tests MINK_DRIVER_ARGS_WEBDRIVER value: '["chrome", { "chromeOptions": { "w3c": false } }, "http://localhost:4444/wd/hub"]' For using the Firefox browser, replace "chrome" with "firefox" -->
    <env name="MINK_DRIVER_ARGS_WEBDRIVER" value=''/>
  </php>
  <testsuites>
    <testsuite name="unit">
      <file>./tests/TestSuites/UnitTestSuite.php</file>
    </testsuite>
    <testsuite name="kernel">
      <file>./tests/TestSuites/KernelTestSuite.php</file>
    </testsuite>
    <testsuite name="functional">
      <file>./tests/TestSuites/FunctionalTestSuite.php</file>
    </testsuite>
    <testsuite name="functional-javascript">
      <file>./tests/TestSuites/FunctionalJavascriptTestSuite.php</file>
    </testsuite>
    <testsuite name="build">
      <file>./tests/TestSuites/BuildTestSuite.php</file>
    </testsuite>
    <testsuite name="passing_kernel">
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Action</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Ajax</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Asset</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Block</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Bootstrap</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Cache</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Command</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Common</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Config</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Database</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Datetime</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/DrupalKernel</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Element</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Extension</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Field</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/File</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Form</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Http</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/HttpKernel</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Image</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Installer</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/KeyValueStore</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Layout</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Lock</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Menu</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Messenger</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Pager</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/ParamConverter</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Path</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Plugin</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Queue</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Render</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/RouteProcessor</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Routing</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/ServiceProvider</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Session</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Site</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/StreamWrapper</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/StringTranslation</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/TempStore</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Test</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Theme</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/TypedData</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Update</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Updater</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Url</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Validation</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Component</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Config</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/RequestProcessing</directory>
      <file>./tests/Drupal/KernelTests/KernelTestBaseTest.php</file>
      <file>./tests/Drupal/KernelTests/KernelTestBaseShutdownTest.php</file>
      <exclude>./tests/Drupal/KernelTests/Core/Validation/UniqueFieldConstraintTest.php</exclude>
    </testsuite>
    <testsuite name="passing_unit">
      <directory suffix="Test.php">tests/Drupal/Tests</directory>
      <directory suffix="Test.php">tests/Drupal/KernelTests/Core/Entity</directory>
    </testsuite>
    <testsuite name="passing_functional">
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Bootstrap</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Breadcrumb</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Core</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Entity</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/HttpKernel</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Image</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Installer</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Routing</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Theme</directory>
      <directory suffix="Test.php">tests/Drupal/FunctionalTests/Update</directory>
    </testsuite>
    <testsuite name="failing_functional_tests">
      <file>./tests/Drupal/FunctionalTests/Hal/BaseFieldOverrideHalJsonAnonTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/BaseFieldOverrideHalJsonBasicAuthTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/BaseFieldOverrideHalJsonCookieTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/DateFormatHalJsonAnonTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/DateFormatHalJsonBasicAuthTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/DateFormatHalJsonCookieTest.php</file>
    </testsuite>
    <testsuite name="failing_functional_tests2">
      <file>./tests/Drupal/FunctionalTests/Hal/EntityFormDisplayHalJsonAnonTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityFormDisplayHalJsonBasicAuthTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityFormDisplayHalJsonCookieTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityFormModeHalJsonAnonTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityFormModeHalJsonBasicAuthTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityFormModeHalJsonCookieTest.php</file>
    </testsuite>
    <testsuite name="failing_functional_tests3">
      <file>./tests/Drupal/FunctionalTests/Hal/EntityViewDisplayHalJsonAnonTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityViewDisplayHalJsonBasicAuthTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityViewDisplayHalJsonCookieTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityViewModeHalJsonAnonTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityViewModeHalJsonBasicAuthTest.php</file>
      <file>./tests/Drupal/FunctionalTests/Hal/EntityViewModeHalJsonCookieTest.php</file>
    </testsuite>
    <testsuite name="intermittent_failures">
      <file>./tests/Drupal/KernelTests/Core/Validation/UniqueFieldConstraintTest.php</file>
    </testsuite>
    <testsuite name="sqlsrv">
      <directory suffix="Test.php">../modules/contrib/sqlsrv</directory>
    </testsuite>
  </testsuites>
  <listeners>
    <listener class="\Drupal\Tests\Listeners\DrupalListener">
    </listener>
    <!-- The Symfony deprecation listener has to come after the Drupal listener -->
    <listener class="Symfony\Bridge\PhpUnit\SymfonyTestsListener">
    </listener>
  </listeners>
  <!-- Filter for coverage reports. -->
  <filter>
    <whitelist>
      <directory>../drivers/lib</directory>
      <!-- Extensions can have their own test directories, so exclude those. -->
      <directory>../modules/contrib</directory>
      <exclude>
        <directory>../modules/*/src/Tests</directory>
        <directory>../modules/*/tests</directory>
        <directory>../modules/*/*/src/Tests</directory>
        <directory>../modules/*/*/tests</directory>
      </exclude>
     </whitelist>
  </filter>
</phpunit>
