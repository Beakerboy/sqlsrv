{
  "name": "drupal/sqlsrv",
  "description": "Driver for SQL Server and SQL Azure",
  "type": "drupal-module",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "David Garcia",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": { "Drupal\\Driver\\Database\\sqlsrv\\": "drivers/lib" }
  },
  "autoload-dev": {
    "psr-4": { "Drupal\\Tests\\sqlsvr\\": "tests/src/" }
  },
  "require": {
    "php" : "^7.0",
    "drupal/core" : "^8.6"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "phpstan/phpstan": "*",
    "php-coveralls/php-coveralls": "*"
  }
}
